      "cell_type": "code",
      "source": [
        "import gspread\n",
        "from google.oauth2.service_account import Credentials\n",
        "import pandas as pd\n",
        "def send_email():\n",
        "      scope = [\"https://www.googleapis.com/auth/spreadsheets\"]\n",
        "\n",
        "      # Load the JSON key file for authentication\n",
        "      creds = Credentials.from_service_account_file('alien-baton-446917-f0-a58499645f4c.json', scopes=scope)\n",
        "\n",
        "      # Authorize the gspread client with the credentials\n",
        "      client = gspread.authorize(creds)\n",
        "\n",
        "      # Extract the spreadsheet ID from the Google Sheets URL\n",
        "      spreadsheet_id = '17JRoVbvBDco-aVBCAzbCMPU6EpGWU0g2ha2KBaaLEWE'\n",
        "\n",
        "      # Open the Google Sheet by its ID\n",
        "      spreadsheet = client.open_by_key(spreadsheet_id)\n",
        "\n",
        "      # Select the first sheet\n",
        "      sheet = spreadsheet.sheet1\n",
        "\n",
        "      # Read the Excel file into a DataFrame\n",
        "      df = pd.read_excel(\"risk_analysis.xlsx\")\n",
        "\n",
        "      # Replace problematic values in the DataFrame\n",
        "      df = df.replace([float('inf'), float('-inf')], None)  # Replace Infinity with None\n",
        "      df = df.fillna('')  # Replace NaN with an empty string\n",
        "\n",
        "      # Convert the DataFrame to a list of lists (including the header row)\n",
        "      data = [df.columns.values.tolist()] + df.values.tolist()\n",
        "\n",
        "      # Clear existing data in the sheet\n",
        "      sheet.clear()\n",
        "\n",
        "      # Update the Google Sheet with the data from the Excel file\n",
        "      sheet.update('A1', data)\n",
        "      return\n",
        "\n",
        "      print(\"Data successfully uploaded to Google Sheets: https://docs.google.com/spreadsheets/d/17JRoVbvBDco-aVBCAzbCMPU6EpGWU0g2ha2KBaaLEWE/edit?gid=0\")"
      ],
